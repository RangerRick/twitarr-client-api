{
  "name": "twitarr",
  "version": "0.1.0",
  "description": "Client API for Twitarr",
  "main": "dist/twitarr.js",
  "bin": {
    "twitarr": "dist/cli.node.js"
  },
  "author": "Benjamin Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RangerRick/twitarr-client-api/issues"
  },
  "homepage": "https://github.com/RangerRick/twitarr-client-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/RangerRick/twitarr-client-api.git"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.6",
    "@types/urijs": "^1.15.36",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "cache-loader": "^1.2.2",
    "chai": "^4.1.2",
    "child_process": "^1.0.2",
    "conventional-changelog-lint": "^2.1.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-environment-node-debug": "^2.0.0",
    "parallel-webpack": "^2.3.0",
    "regenerator-runtime": "^0.13.1",
    "standard-changelog": "^1.0.13",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "3.5.4",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^2.7.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "urijs": "^1.19.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.1.5",
    "webpack-closure-compiler": "^2.1.6",
    "yargs": "^12.0.0",
    "yarn": "^1.13.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "cli-table2": "^0.2.0",
    "commander": "^2.19.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.startcase": "^4.4.0",
    "moment": "^2.24.0",
    "qs": "^6.5.0",
    "xmldom": "^0.1.27"
  },
  "resolutions": {
    "typescript": "^2.x"
  },
  "scripts": {
    "precommit": "jest",
    "prepush": "conventional-changelog-lint --from=origin/master && yarn --ignore-engines lint",
    "changelog": "standard-changelog -i CHANGELOG.md -s -r 1",
    "build": "webpack --define process.env.NODE_ENV=production --env production",
    "dev": "webpack --progress --colors",
    "dist": "yarn --ignore-engines lint && yarn --ignore-engines test && yarn --ignore-engines build",
    "docs": "webpack --env docs",
    "watch": "webpack --progress --colors --watch",
    "test": "jest",
    "watch-test": "jest --watch",
    "lint": "tslint 'src/**/*.ts'",
    "cli": "ts-node src/CLI.ts",
    "docker-build": "yarn --ignore-engines && yarn --ignore-engines lint && yarn --ignore-engines test && yarn --ignore-engines  build && yarn --ignore-engines changelog && ./fix-ownership.sh",
    "docker": "docker run --rm --name twitarr-js -v `pwd`:/build -w /build node:carbon yarn --ignore-engines docker-build"
  }
}
