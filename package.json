{
  "name": "twitarr",
  "version": "0.1.0",
  "description": "Client API for Twitarr",
  "main": "dist/twitarr.js",
  "bin": {
    "twitarr": "dist/cli.node.js"
  },
  "author": "Benjamin Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RangerRick/twitarr-client-api/issues"
  },
  "homepage": "https://github.com/RangerRick/twitarr-client-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/RangerRick/twitarr-client-api.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.24",
    "@types/urijs": "^1.15.38",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "cache-loader": "^2.0.1",
    "chai": "^4.2.0",
    "child_process": "^1.0.2",
    "conventional-changelog-lint": "^2.1.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^24.1.0",
    "jest-environment-node-debug": "^2.0.0",
    "parallel-webpack": "^2.3.0",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1",
    "standard-changelog": "^1.0.13",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "urijs": "^1.19.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-closure-compiler": "^2.1.6",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "buffer": "^5.2.1",
    "cli-table2": "^0.2.0",
    "form-data": "^2.3.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.startcase": "^4.4.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.3.0",
    "qs": "^6.6.0",
    "word-wrap": "^1.2.3",
    "xmldom": "^0.1.27"
  },
  "resolutions": {
    "typescript": "^3.x"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "test/**/*.{js,jsx,ts,tsx}",
      "!src/API.ts",
      "!src/CLI.ts",
      "!test/rest/MockHTTP.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "dist/coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/dist/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testURL": "http://localhost"
  },
  "scripts": {
    "precommit": "jest",
    "prepush": "conventional-changelog-lint --from=origin/master && npm run lint",
    "changelog": "standard-changelog -i CHANGELOG.md -s -r 1",
    "build": "webpack --define process.env.NODE_ENV=production --env production",
    "dev": "webpack --progress --colors",
    "dist": "npm run lint && npm run test && npm run build",
    "docs": "webpack --env docs",
    "watch": "webpack --progress --colors --watch",
    "test": "jest",
    "watch-test": "jest --watch",
    "lint": "eslint 'src/**/*.ts' && eslint 'test/**/*.ts'",
    "cli": "ts-node src/CLI.ts",
    "docker-build": "npm install && npm run lint && npm run test && npm run build && npm run changelog && ./fix-ownership.sh",
    "docker": "docker run --rm --name twitarr-js -v `pwd`:/build -w /build node:carbon npm run docker-build"
  }
}
